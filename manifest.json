{
  "manifest_version": 2,

  "name": "Chrome Tab List",
  "description": "Lists chrome tabs",
  "version": "1.0.0.0",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "contextMenus",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "icons":{
  },
  "background":{
     "scripts": [ "js/background.js"]
  }
}